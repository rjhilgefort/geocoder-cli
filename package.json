{
  "name": "geocoder-cli",
  "version": "1.0.0",
  "description": "geocoder-cli",
  "main": "app.js",
  "keywords": [
    "geocode",
    "geocoder",
    "google",
    "maps",
    "cli"
  ],
  "author": "rjhilgefort@gmail.com",
  "license": "ISC",
  "scripts": {
    "start": "npm run lint && node app",
    "test": "jest --watch -o --verbose",
    "lint": "eslint --ext .js ./src",
    "lint-fix": "eslint --ext .js ./src --fix"
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^20.0.3",
    "jest": "^20.0.4"
  },
  "eslintConfig": {
    "env": {
      "jest": true
    },
    "extends": [
      "airbnb-base"
    ],
    "plugins": [
      "jest"
    ],
    "rules": {
      "no-unused-vars": 1,
      "comma-dangle": [
        "error",
        "never"
      ],
      "no-underscore-dangle": 0,
      "no-mixed-operators": 0,
      "arrow-parens": 0
    }
  },
  "dependencies": {
    "dotenv": "^4.0.0"
  }
}
